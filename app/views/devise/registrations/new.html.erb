<div class="jumbotron">
<div class="container">
  <div class="row top-buffer"></div>
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">

<div class="login-container" >
  <h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
    <%= f.email_field :email, autofocus: true, placeholder: "Email", class: "form-control" %><br>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password - Miminum Six Characters", class: "form-control" %><br>
    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Password Confirmation",class: "form-control" %>
    <br>

  <div class="btn-group">
    <%= f.submit "Sign up!", class: "btn btn-default center-block" %>
  </div>
<% end %>
</div>
</div>
<div class="col-md-3"></div>
</div>
</div>
